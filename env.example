# Environment Variables for N8N Headline Image Generator

# N8N Webhook URL (Required for production)
# This webhook receives email, keyword, and image_url, then generates headline images
VITE_WEBHOOK_URL=https://hungdang2.app.n8n.cloud/webhook/imgheadline

# Optional: Custom API timeout (in milliseconds)
VITE_API_TIMEOUT=30000

# Optional: Enable debug mode (set to true for development)
VITE_DEBUG=true 